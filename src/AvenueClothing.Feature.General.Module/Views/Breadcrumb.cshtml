@model AvenueClothing.Feature.General.Module.ViewModels.BreadcrumbWrapper
<ol class="breadcrumb">
    @foreach (var item in Model.SitecoreBreadcrumbs)
    {
        <li class="breadcrumb-item">
            @if (@item.IsActive)
            { @item.BreadcrumbName}
            else
            {
                <a href="@item.BreadcrumbUrl">
                    @item.Name
                </a>
            }
        </li>
        }

    @foreach (var item in Model.UcommerceBreadcrumbs)
    {
        <li class="breadcrumb-item">
                <a href="@item.BreadcrumbUrlUcommerce">
                    @item.BreadcrumbNameUcommerce
                </a>
            
        </li>
    }

</ol>
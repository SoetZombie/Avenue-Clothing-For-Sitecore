    <component id="AvenueClothing.InstallationPipeline.RunScratchIndexer"
       service="UCommerce.Pipelines.IPipelineTask`1[[AvenueClothing.Foundation.Installer.Pipelines.Installation.InstallationPipelineArgs, AvenueClothing.Foundation.Installer]], UCommerce"
       type="AvenueClothing.Foundation.Installer.Pipelines.Installation.Tasks.RunScratchIndexerTask, AvenueClothing.Foundation.Installer" />

		<component id="AvenueClothing.InstallationPipeline.SynchronizeSitecoreItems"
       service="UCommerce.Pipelines.IPipelineTask`1[[AvenueClothing.Foundation.Installer.Pipelines.Installation.InstallationPipelineArgs, AvenueClothing.Foundation.Installer]], UCommerce"
       type="AvenueClothing.Foundation.Installer.Pipelines.Installation.Tasks.SynchronizeSitecoreItemsTask, AvenueClothing.Foundation.Installer">

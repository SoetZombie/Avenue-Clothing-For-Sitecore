				   service="UCommerce.Pipelines.IPipeline`1[[AvenueClothing.Foundation.Installer.Pipelines.Installation.InstallationPipelineArgs, AvenueClothing.Foundation.Installer]], UCommerce"
				   type="AvenueClothing.Foundation.Installer.Pipelines.Installation.InstallationPipeline, AvenueClothing.Foundation.Installer">

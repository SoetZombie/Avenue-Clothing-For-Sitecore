    <component id="AvenueClothing.InstallationPipeline.RenameWebConfigIfMissing"
       service="UCommerce.Pipelines.IPipelineTask`1[[AvenueClothing.Foundation.Installer.Pipelines.Installation.InstallationPipelineArgs, AvenueClothing.Foundation.Installer]], UCommerce"
